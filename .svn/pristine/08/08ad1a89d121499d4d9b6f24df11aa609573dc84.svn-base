          <div class="col">

            <!-- breacrumb -->
            <nav aria-label="breadcrumb">
              <ol class="breadcrumb">
                <li class="breadcrumb-item active"><a href="#">FEWS</a></li>
                <li class="breadcrumb-item active">
                  Workflows
                </li>
                <li class="breadcrumb-item active" aria-current="page">
                  <span id="workflowImportStatus" title="Geladene Taskruns">
                    <span class="badge badge-success">laden</span> 
                  </span>
                </li>
              </ol>
            </nav>



            <!-- Icon Cards-->
            <div class="row">
              <div class="col-xl-3 col-sm-6 mb-3">
                <div  class="card pointer text-white bg-success o-hidden h-100" id="card_fertig" title="Taskruns mit Status 'Completed fully successfull' und 'Approved'">
                  <div class="card-body">
                    <div class="card-body-icon">
                      <i class="fa fa-fw fa-check"></i>
                    </div>
                    <div class="mr-5 text-center"><span class="display-4" id="fews_fertig">..</span></div>
                  </div>
                  <span class="card-footer text-white clearfix  z-1 text-center" href="#">
                    <span class="text-uppercase">Abgeschlossen</span>
                    <!-- <span >davon <span id="fews_approved">..</span> bestätigt</span> -->
                  </span>                  
                </div>
              </div>
              <div class="col-xl-3 col-sm-6 mb-3">
                <div class="card pointer text-white bg-primary o-hidden h-100"  id="card_laufend" title="Taskruns mit Status 'Approved partly successful' und 'Completed partly successful'">
                  <div class="card-body">
                    <div class="card-body-icon">
                      <i class="fa fa-fw fa-clock-o"></i>
                    </div>
                    <div class="mr-5 text-center"><span class="display-4 " id="fews_laufend">..</span>  </div>
                  </div>
                  <span class="card-footer text-white clearfix  z-1 text-center" href="#">
                    <span class="text-uppercase">Laufend/Ausstehend</span>
                  </span>    
                </div>
              </div>
              <div class="col-xl-3 col-sm-6 mb-3">
                <div class="card pointer text-white bg-warning o-hidden h-100"  id="card_warnung" title="Taskruns mit Status 'Running' und 'Pending'">
                  <div class="card-body">
                    <div class="card-body-icon">
                      <i class="fa fa-fw fa-exclamation-triangle"></i>
                    </div>
                    <div class="mr-5 text-center"><span class="display-4 " id="fews_warnungen">..</span> </div>
                  </div>
                  <span class="card-footer text-white clearfix  z-1 text-center" href="#">
                    <span class="text-uppercase">Warnungen</span>
                  </span>    
                </div>
              </div>
              <div class="col-xl-3 col-sm-6 mb-3">
                <div class="card pointer text-white bg-danger o-hidden h-100"  id="card_fehler" title="Taskruns mit Status 'Invalid','Terminated' und 'Failed'">
                  <div class="card-body">
                    <div class="card-body-icon">
                      <i class="fa fa-fw fa-times-circle-o"></i>
                    </div>
                    <div class="mr-5 text-center"><span class="display-4" id="fews_fehler">..</span> </div>
                  </div>
                  <span class="card-footer text-white clearfix  z-1 text-center" href="#">
                    <span class="text-uppercase">Fehler</span>
                  </span>    
                </div>
              </div>
            </div>
            
           <!-- card Body -->
            <div class="card" id="tableHide">
              <div class="card-header  text-uppercase"><i class="fa fa-table"></i> Delft-FEWS Workflow Overview</div>
              
              <!-- chartjs -->
              <div class="card-body">

                <!-- <h5 class="card-title  text-uppercase">Systemstatus [DEMO]</h5> QualitativePrognose_Verbaende -->
                <div class="chart-container">
                  <canvas id="myChart" width="400" height="200"></canvas>
                </div>  

                <!-- input Feld um Tage zu aktualisieren -->
                <div class="container">
                    <div class="row">
                        <div class="col-md-4"></div>
                        <div class="col-md-4" title="Ladezeitraum ändern">   
                          <div class="col-xs-3 col-xs-offset-3">
                            <div class="input-group  input-group-sm number-spinner">
                              <span class="input-group-btn">
                                <button class="btn btn-sm btn-light" data-dir="dwn"><span class="fa fa-minus"></span></button>
                              </span>
                              <input type="text" id="chartDaySetter"  class="form-control text-center">
                              <span class="input-group-btn">
                                <button class="btn btn-sm btn-light" data-dir="up"><span class="fa fa-plus"></span></button>
                              </span>
                            </div>
                          </div>
                        </div>
                        <div class="col-md-4"></div>
                    </div>
                  </div>

              </div>
              
              <!--FEWS Tabelle-->  

              <div class="card-body">
                <h5 class="card-title  text-uppercase">Workflowstatus <i id="tableLoaderSpinner" class="fa fa-spin fa-spinner" aria-hidden="true"></i></h5>
                <p class="card-text">&Uuml;bersicht &uuml;ber die Workflows aus dem Delft-FEWS System: <span class="filterBeschreibung"></span></p> 
                <div class="table-responsive">
                  <table class="table display table-bordered" id="dataTable" width="100%" cellspacing="0">
                    <thead>
                      <tr>
                        <th>Workflow</th>
                        <th>Dispatch Time</th>
                        <th title="Abgeschlossen bzw. zuletzt aktualisiert in der Datenbank">Zeitstempel</th>
                        <th>Status</th>
                      </tr>
                    </thead>
                    <tbody>
                        <tr class="table-secondary odd" role="row"><td class="sorting_1">Laden....</td><td class="sorting_1">nicht abgeschlossen</td><td class="sorting_1">nicht abgeschlossen</td><td class="sorting_1">running</td></tr>
                    </tbody>
                  </table>
                </div>
              </div>
              <div class="card-footer small text-muted" id="tableNotes">Zustandspeicherung aktiviert</div>
            </div>
          </div>
        </div>
      </div>
